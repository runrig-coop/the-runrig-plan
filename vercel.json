{
  "cleanUrls": true,
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/posts/hedgerows(.*)",
      "destination": "/journal/hedgerows",
      "permanent": true
    },
    {
      "source": "/overview(.*)",
      "destination": "/plan/overview",
      "permanent": true
    },
    {
      "source": "/architecture(.*)",
      "destination": "/plan/architecture",
      "permanent": true
    },
    {
      "source": "/ecology(.*)",
      "destination": "/plan/ecology",
      "permanent": true
    },
    {
      "source": "/roadmap-2024(.*)",
      "destination": "/archive/roadmap-2024",
      "permanent": true
    }
  ]
}