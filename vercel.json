{
  "cleanUrls": true,
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/contact(.*)",
      "destination": "/about",
      "permanent": true
    },
    {
      "source": "/journal/hedgerows(.*)",
      "destination": "/posts/hedgerows",
      "permanent": true
    },
    {
      "source": "/overview(.*)",
      "destination": "/posts/the-runrig-plan-for-socio-ecological-design",
      "permanent": true
    },
    {
      "source": "/architecture(.*)",
      "destination": "/posts/the-runrig-plan-appendix-architecture",
      "permanent": true
    },
    {
      "source": "/ecology(.*)",
      "destination": "/posts/the-runrig-plan-appendix-ecology",
      "permanent": true
    },
    {
      "source": "/roadmap-2024(.*)",
      "destination": "/posts/roadmap-2024",
      "permanent": true
    }
  ]
}